[
    {"$assemble": {
        "erd": "./assets/schema1.json",
        "entity": "Customer",
        "project": ["customerAddress", "customerName"],
        "subassemble": [{
            "entity": "Account",
            "join": "inner",
            "project": ["accountName"],
            "subassemble": [{
                "entity": "Order",
                "join": "inner",
                "project": ["amount"],
                "filter": { "$gte": ["$Order.amount", 100] }
            }]
        }]
    }},
    {"$limit": 10},
    {"$skip": 0}
]
