[
    {"$conjure": [
        "Customer.last_name",
        "Order._id",
        "Order.total_amount",
        "OrderItem.*",
        "Category.*"
    ]},
    {"$match": {"$expr": {"$gte": ["$Customer.last_name", "fish"]}}},
    {"$sort": {"Order.total_amount": -1}},
    {"$limit": 10},
    {"$skip": 0}
]
